{
  // Core
  "window.autoDetectColorScheme": true,
  "window.commandCenter": false,
  "window.restoreWindows": "none",
  "window.confirmSaveUntitledWorkspace": false,
  "window.menuBarVisibility": "compact",

  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.limit.value": 5,
  "workbench.iconTheme": "file-icons",
  "workbench.layoutControl.enabled": false,
  "workbench.list.smoothScrolling": true,
  "workbench.productIconTheme": "icons-carbon",
  "workbench.startupEditor": "none",
  "workbench.tree.indent": 10,
  "workbench.tree.enableStickyScroll": false,

  "editor.cursorBlinking": "smooth",
  "editor.cursorSurroundingLines": 2,
  "editor.dragAndDrop": false,
  "editor.find.addExtraSpaceOnTop": false,
  "editor.fontFamily": "Iosevka, '等距更纱黑体 SC'",
  "editor.fontLigatures": true,
  "editor.linkedEditing": true,
  "editor.minimap.renderCharacters": false,
  "editor.stickyScroll.enabled": false,
  "editor.tabSize": 2,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.rulers": [
    { "color": "#ffffff10", "column": 80 },
    { "color": "#ffffff20", "column": 100 },
    { "color": "#ffffff30", "column": 120 }
  ],
  "editor.experimentalWhitespaceRendering": "font",
  "editor.inlayHints.enabled": "offUnlessPressed",
  "editor.occurrencesHighlight": "multiFile",
  "editor.occurrencesHighlightDelay": 0,
  "editor.renderWhitespace": "boundary",
  "editor.smoothScrolling": true,
  "editor.gotoLocation.multipleDefinitions": "goto",

  "files.autoSave": "onFocusChange",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.simpleDialog.enable": true,
  "search.collapseResults": "auto",
  "diffEditor.hideUnchangedRegions.enabled": true,
  "explorer.autoReveal": "focusNoScroll",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorWidth": 2,
  "terminal.integrated.defaultProfile.windows": "Command Prompt",
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "terminal.integrated.fontFamily": "'Cascadia Code', '等距更纱黑体 SC', monospace",
  "terminal.integrated.suggest.enabled": true,
  "scm.diffDecorationsGutterWidth": 2,
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "extensions.ignoreRecommendations": true,
  "chat.commandCenter.enabled": false,

  // Builtin Extensions
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.untrackedChanges": "separate",
  "git.blame.statusBarItem.enabled": true,
  "debug.javascript.codelens.npmScripts": "never",
  "typescript.preferences.quoteStyle": "single",
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.disableAutomaticTypeAcquisition": true,
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.formatOnSave": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.formatOnSave": true
  },

  // Third-party Extensions
  "cSpell.enabled": false,
  "cSpell.allowCompoundWords": true,
  "cSpell.userWords": ["hyrious"],
  "prettier.enable": false,
  "eslint.useFlatConfig": true,
  "emmet.showSuggestionsAsSnippets": true,
  "todohighlight.keywordsPattern": "(?:TODO|FIXME|XXX):|--- (?:Start|End) \\w+ ---",
  "todohighlight.defaultStyle": {
    "color": "#66CCFF",
    "backgroundColor": "#ADD8E640",
    "overviewRulerColor": "#66CCFF"
  },
  "githubPullRequests.pullBranch": "never",
  "githubPullRequests.pushBranch": "always",
  "githubPullRequests.fileListLayout": "flat",
  "github.copilot.chat.agent.thinkingTool": true,
  "github.copilot.nextEditSuggestions.enabled": false,
  "python.analysis.typeCheckingMode": "off",
  "moonbit.inlayHints.forAnonymousFunctionReturnTypes": false,
  "moonbit.inlayHints.forBinderTypes": false,
  "moonbit.ai.telemetry": "on",

